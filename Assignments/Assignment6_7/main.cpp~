#include<iostream>
#include<fstream>
#include <rarray>
#include <rarrayio>
#include <complex>

int main(){
  // open the file
  std::ifstream f("/home/b/bovy/bahmanya/Assignments/hw6_7/gwdata/GWprediction.rat");
  // create empty arrays
  rarray<double,1> times;
  rarray<std::complex<double>,1> signal;
  // read in the signal
  f >> times;
  f >> signal;
  //std::cout << "Here it is:" << times << "\n" << std::endl;
  std::cout << "size is:" << sizeof(times) << std::endl;

}
